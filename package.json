  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@parcel/transformer-glsl": "^2.0.0-nightly.2273",
    "@parcel/transformer-image": "^2.0.0-nightly.2273",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "babel-core": "^7.0.0-0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.2",
    "nodemon": "^2.0.7",
    "parcel": "^2.0.0-nightly.649",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "framer-motion": "^4.1.11",
    "jest": "^26.6.3",
    "lodash.clonedeep": "^4.5.0",
    "path-browserify": "^1.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-colorful": "^5.1.4",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.3.1",
    "reload": "^3.1.1",
    "styled-components": "^5.3.0",
    "ts-jest": "^26.5.6",
    "twgl.js": "^4.19.1",
    "use-undo": "^1.0.3"
  },
  "alias": {
    "react-contextmenu": "./src/engine/editor/app/components/react-contextmenu"
  },
  "prettier": {
    "printWidth": 100
  },
  "staticFiles": {
    "staticPath": "assets/",
    "staticOutPath": "assets/",
    "distDir": "dist/"
